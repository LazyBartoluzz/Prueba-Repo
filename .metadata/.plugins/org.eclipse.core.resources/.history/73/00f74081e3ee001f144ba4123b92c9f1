package demo;

public class Main {

	public static void main(String[] args) {
		
		
		System.out.println("git config --global user.name \"name\"");
        System.out.println("git config --global user.email \"email\"");
        System.out.println("git config --list");
        System.out.println("git init");
        System.out.println("git status");
        System.out.println("git log");
        System.out.println("git add.");
        System.out.println("git config --list");
        System.out.println("git commit");
        System.out.println("git push");
        System.out.println("git pull");
        System.out.println("git switch");
        System.out.println("git branch -D");
        System.out.println("git branch --delete");
        System.out.println("git merge");
	}
}
